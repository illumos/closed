: .ipaddr_t ( addr -- )
	." [ "
	dup d# 24 >> .d 
	." . "
	dup d# 16 >> h# ff and .d
	." . "                
	dup d# 8 >> h# ff and .d 
	." . "               
	h# ff and .d            
	." ]"
;


